<?xml version="1.0" encoding="UTF-8"?>
<chat:BottomContorollerMenuViewBase
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="ChatSample.Views.CustomBottomMenuView"
    xmlns:local="clr-namespace:ChatSample"
    xmlns:chat="clr-namespace:Entap.Chat;assembly=Entap.Chat">
    <ContentView.Content>
        <StackLayout
            Spacing="0"
            Orientation="Horizontal">
            <Button
                x:Name="SendPhotoButton"
                Margin="10,0,5,0"
                WidthRequest="48"
                HeightRequest="48"
                Text="カメラ"
                BackgroundColor="Red"
                VerticalOptions="EndAndExpand"
                TextColor="White"/>
        </StackLayout>
    </ContentView.Content>
</chat:BottomContorollerMenuViewBase>
